{
  "name": "@cs125/gatsby-theme-cs125-docs",
  "license": "MIT",
  "version": "2020.5.0",
  "main": "index.js",
  "scripts": {
    "backend": "docker-compose --env-file .env.development -p $npm_package_name up",
    "check": "npm run tsc && npm run prettier",
    "clean": "gatsby clean",
    "start": "concurrently -n example,backend \"gatsby develop -p 1234\" \"npm run backend\"",
    "tsc": "tsc",
    "prettier": "prettier \"**/*{.tsx,.ts,.js,.json,.md,.mdx}\" --write && prettier-package-json --write",
    "ncu": "ncu"
  },
  "dependencies": {
    "@cs125/element-tracker": "^2020.5.6",
    "@cs125/semantic-ui": "^2020.5.17",
    "@mdx-js/mdx": "^1.6.1",
    "@mdx-js/react": "^1.6.1",
    "gatsby-plugin-mdx": "^1.2.8",
    "gatsby-plugin-preload-fonts": "^1.2.3",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-source-filesystem": "^2.3.4",
    "runtypes": "^4.2.0",
    "semantic-ui-react": "^0.88.2"
  },
  "peerDependencies": {
    "gatsby": "^2.21.37",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/mdx-js__react": "^1.5.2",
    "concurrently": "^5.2.0",
    "gatsby": "^2.21.37",
    "npm-check-updates": "^6.0.1",
    "prettier": "^2.0.5",
    "prettier-package-json": "^2.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.2"
  },
  "prettier": {
    "semi": false,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2
  }
}
